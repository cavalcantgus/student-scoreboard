      student(newValue){\r\n            this.localStudent = { ...newValue}\r\n        }\r\n    },\r\n    methods: {\r\n        gradesValidation(gradeNumber){\r\n            return [\r\n                value => !!value || `Você deve digitar a nota ${gradeNumber}`,\r\n                value => (value && !isNaN(value) && value >= 0 && value <= 10) || `A nota ${gradeNumber} deve ser um número entre 0 e 10`\r\n            ]\r\n        },\r\n        close() {\r\n            this.localVisible = false; // Fecha o diálogo\r\n            this.$emit('cancel'); // Emite o evento de cancelamento\r\n        },\r\n        async save() {\r\n            const { valid } = await this.$refs.form.validate();\r\n            if (valid) {\r\n                this.$emit('save', this.localStudent);\r\n                this.close(); // Fecha o diálogo após salvar\r\n        }\r\n        },\r\n         outsideClick() {\r\n            this.localVisible = false\r\n         }\r\n    }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.custom-card {\r\n    border-radius: 76px;\r\n}\r\n.card-title {\r\n    text-align: center;\r\n    width: 100%; \r\n    display: block; \r\n    margin-bottom: 16px; \r\n    font-size: 20px; \r\n    font-weight: bold; \r\n}\r\n.save-button {\r\n    background-color: rgb(70, 70, 245);\r\n    color: #fff !important;\r\n}\r\n.cancel-button {\r\n    background: rgb(235, 48, 48);\r\n    color: #fff !important;\r\n}\r\n</style>\r\n"],"mappings":"AAyCA,eAAe;EACXA,KAAK,EAAE;IACHC,OAAO,EAAEC,MAAM;IACfC,OAAO,EAAEC;EACb,CAAC;EACDC,IAAIA,CAAA,EAAG;IACH,OAAM;MACFC,YAAY,EAAE;QAAE,GAAG,IAAI,CAACL;MAAO,CAAC;MAChCM,YAAY,EAAE,IAAI,CAACJ,OAAO;MAC1BK,KAAK,EAAE;QACHC,IAAI,EAAEC,KAAI,IAAK,CAAC,CAACA,KAAI,IAAK;MAC9B;IACJ;EACJ,CAAC;EACDC,KAAK,EAAE;IACHR,OAAOA,CAACS,QAAQ,EAAE;MACd,IAAI,CAACL,YAAW,GAAIK,QAAO;IAC/B,CAAC;IACDX,OAAOA,CAACW,QAAQ,EAAC;MACb,IAAI,CAACN,YAAW,GAAI;QAAE,GAAGM;MAAQ;IACrC;EACJ,CAAC;EACDC,OAAO,EAAE;IACLC,gBAAgBA,CAACC,WAAW,EAAC;MACzB,OAAO,CACHL,KAAI,IAAK,CAAC,CAACA,KAAI,IAAK,4BAA4BK