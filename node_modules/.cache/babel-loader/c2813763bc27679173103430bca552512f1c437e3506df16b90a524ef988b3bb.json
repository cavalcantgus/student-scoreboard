mplate>\r\n            <template v-slot:item.grade4=\"{ item }\">\r\n                {{ item.grade4 }}\r\n            </template>\r\n            <template v-slot:item.average=\"{ item }\">\r\n                {{ calculateAverage(item) }}\r\n            </template>\r\n            <template v-slot:item.status=\"{ item }\">\r\n                {{ calculateStatus(item) }}\r\n            </template>\r\n            <template v-slot:item.actions=\"{ item }\">\r\n                <v-icon @click=\"editStudent(item)\">mdi-pencil</v-icon>\r\n                <v-icon @click=\"deleteStudent(item)\">mdi-delete</v-icon>\r\n            </template>\r\n        </v-data-table>\r\n\r\n        <StudentForm \r\n        :student=\"currentStudent\"\r\n        :visible=\"formVisible\"\r\n        @save=\"save\"\r\n        @cancel=\"cancel\"\r\n        />\r\n    </div>\r\n</template>\r\n\r\n\r\n<script>\r\n\r\nimport StudentForm from './StudentForm.vue'\r\n\r\nexport default {\r\n   components: {\r\n    StudentForm\r\n   },\r\n   data() {\r\n    return{\r\n        headers: [\r\n            { title: 'Nome', value: 'name', class: 'name-column' },\r\n            { title: 'Nota 1', value: 'grade1', class: 'grade-column' },\r\n            { title: 'Nota 2', value: 'grade2', class: 'grade-column' },\r\n            { title: 'Nota 3', value: 'grade3', class: 'grade-column' },\r\n            { title: 'Nota 4', value: 'grade4', class: 'grade-column' },\r\n            { title: 'Média', value: 'average', class: 'grade-column' },\r\n            { title: 'Situação', value: 'status', class: 'status-column' },\r\n            { title: 'Ações', value: 'actions', sortable: false, class: 'actions-column' }\r\n        ],\r\n        students: [],\r\n        currentStudent: {},\r\n        formVisible: false\r\n    }\r\n   },\r\n   methods: {\r\n    showForm(student = {}) {\r\n        this.currentStudent = {...student}\r\n        this.formVisible = true\r\n    },\r\n    closeForm () {\r\n        this.formVisible = false\r\n    },\r\n    save(student){\r\n        if(student.id) {\r\n            const index = this.students.findIndex(s => s.id === student.id)\r\n            if(index !== -1){\r\n                this.students[index] = student\r\n            }\r\n        } else{\r\n            student.id = Date.now()\r\n            this.students.push(student)\r\n        }\r\n        this.closeForm()\r\n    },\r\n    editStudent(student) {\r\n      this.showForm(student)\r\n    },\r\n    deleteStudent(student) {\r\n      this.students = this.students.filter(s => s.id !== student.id)\r\n    },\r\n    calculateAverage(student) {\r\n      const grades = [student.grade1, student.grade2, student.grade3, student.grade4].map(Number)\r\n      const total = grades.reduce((sum, grade) => sum + grade, 0)\r\n      return (total / grades.length).toFixed(2)\r\n    },\r\n    calculateStatus(student) {\r\n      return this.calculateAverage(student) >= 7 ? 'Approved' : 'Failed'\r\n    }\r\n   }\r\n}\r\n</script>\r\n\r\n<style>\r\n.v-data-table-header th {\r\n  padding: 50px; /* Aumenta o padding nas células do cabeçalho */\r\n}\r\n\r\n.v-data-table tbody td {\r\n  padding: 20px; /* Aumenta o padding nas células do corpo da tabela */\r\n}\r\n\r\n.v-data-table .name-column {\r\n  width: 200px; /* Definir uma largura personalizada */\r\n}\r\n\r\n.v-data-table .grade-column {\r\n  width: 100px;\r\n}\r\n\r\n.v-data-table .status-column {\r\n  width: 150px;\r\n}\r\n</style>"],"mappings":";;;;;;uBACIA,mBAAA,CAqCM,cApCFC,YAAA,CAA0CC,gBAAA;IAAlCC,OAAK,EAAEC,QAAA,CAAAC;EAAQ;IAF/BC,OAAA,EAAAC,QAAA,CAEiC,MAAS,CAF1CC,gBAAA,CAEiC,WAAS,E;IAF1CC,CAAA;kCAIQR,YAAA,CA0BeS,uBAAA;IA1BAC,OAAO,EAAEC,KAAA,CAAAD,OAAO;IAAGE,KAAK,EAAED,KAAA,CAAAE;;IACpB,WAAS,EAAAP,QAAA,CACtB,CAAe;MADWQ;IAAI,OAL9CP,gBAAA,CAAAQ,gBAAA,CAMmBD,IAAI,CAACE,IAAI,iB;IAEC,aAAW,EAAAV,QAAA,CACxB,CAAiB;MADWQ;IAAI,OARhDP,gBAAA,CAAAQ,gBAAA,CASmBD,IAAI,CAACG,MAAM,iB;IAED,aAAW,EAAAX,QAAA,CACxB,CAAiB;MADWQ;IAAI,OAXhDP,gBAAA,CAAAQ,gBAAA,CAYmBD,IAAI,CAACI,MAAM,iB;IAE