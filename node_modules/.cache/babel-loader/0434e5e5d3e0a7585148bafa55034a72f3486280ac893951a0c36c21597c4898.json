{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport StudentForm from './StudentForm.vue';\nexport default {\n  components: {\n    StudentForm\n  },\n  data() {\n    return {\n      showForm: false,\n      currentStudent: null,\n      students: [],\n      headers: [{\n        text: 'Name',\n        value: 'name'\n      }, {\n        text: 'Grade 1',\n        value: 'grade1'\n      }, {\n        text: 'Grade 2',\n        value: 'grade2'\n      }, {\n        text: 'Grade 3',\n        value: 'grade3'\n      }, {\n        text: 'Grade 4',\n        value: 'grade4'\n      }, {\n        text: 'Average',\n        value: 'average'\n      }, {\n        text: 'Status',\n        value: 'status'\n      }, {\n        text: 'Actions',\n        value: 'actions'\n      }]\n    };\n  },\n  methods: {\n    calculateAverage(student) {\n      const grades = [student.grade1, student.grade2, student.grade3, student.grade4].map(Number);\n      const total = grades.reduce((sum, grade) => sum + grade, 0);\n      return total / grades.length;\n    },\n    calculateStatus(student) {\n      return this.calculateAverage(student) >= 7 ? 'Approved' : 'Failed';\n    },\n    editStudent(student) {\n      this.currentStudent = {\n        ...student\n      };\n      this.showForm = true;\n    },\n    deleteStudent(studentId) {\n      this.students = this.students.filter(student => student.id !== studentId);\n    },\n    handleSave(student) {\n      if (student.id) {\n        // Update existing student\n        const index = this.students.findIndex(stu => stu.id === student.id);\n        this.$set(this.students, index, student);\n      } else {\n        // Add new student\n        student.id = Date.now(); // Simple unique ID\n        this.students.push(student);\n      }\n      this.showForm = false;\n    }\n  }\n};","map":{"version":3,"names":["StudentForm","components","data","showForm","currentStudent","students","headers","text","value","methods","calculateAverage","student","grades","grade1","grade2","grade3","grade4","map","Number","total","reduce","sum","grade","length","calculateStatus","editStudent","deleteStudent","studentId","filter","id","handleSave","index","findIndex","stu","$set","Date","now","push"],"sources":["C:\\Temp\\ws-vscode\\student-score-board\\src\\components\\StudentTable.vue"],"sourcesContent":["<template>\r\n  <v-container>\r\n    <v-data-table :headers=\"headers\" :items=\"students\" item-key=\"id\">\r\n      <template v-slot:items=\"props\">\r\n        <tr :key=\"props.item.id\">\r\n          <td>{{ props.item.name }}</td>\r\n          <td>{{ props.item.grade1 }}</td>\r\n          <td>{{ props.item.grade2 }}</td>\r\n          <td>{{ props.item.grade3 }}</td>\r\n          <td>{{ props.item.grade4 }}</td>\r\n          <td>{{ calculateAverage(props.item) }}</td>\r\n          <td>{{ calculateStatus(props.item) }}</td>\r\n          <td>\r\n            <v-btn @click=\"editStudent(props.item)\">Edit</v-btn>\r\n            <v-btn @click=\"deleteStudent(props.item.id)\">Delete</v-btn>\r\n          </td>\r\n        </tr>\r\n      </template>\r\n    </v-data-table>\r\n\r\n    <!-- Botão para adicionar um novo aluno -->\r\n    <v-btn @click=\"showForm = true\">Add Student</v-btn>\r\n\r\n    <!-- Componente de Formulário de Aluno -->\r\n    <student-form\r\n      v-if=\"showForm\"\r\n      :student=\"currentStudent\"\r\n      @save=\"handleSave\"\r\n      @cancel=\"showForm = false\"\r\n    />\r\n  </v-container>\r\n</template>\r\n\r\n<script>\r\nimport StudentForm from './StudentForm.vue';\r\n\r\nexport default {\r\n  components: {\r\n    StudentForm\r\n  },\r\n  data() {\r\n    return {\r\n      showForm: false,\r\n      currentStudent: null,\r\n      students: [],\r\n      headers: [\r\n        { text: 'Name', value: 'name' },\r\n        { text: 'Grade 1', value: 'grade1' },\r\n        { text: 'Grade 2', value: 'grade2' },\r\n        { text: 'Grade 3', value: 'grade3' },\r\n        { text: 'Grade 4', value: 'grade4' },\r\n        { text: 'Average', value: 'average' },\r\n        { text: 'Status', value: 'status' },\r\n        { text: 'Actions', value: 'actions' }\r\n      ]\r\n    };\r\n  },\r\n  methods: {\r\n    calculateAverage(student) {\r\n      const grades = [student.grade1, student.grade2, student.grade3, student.grade4].map(Number);\r\n      const total = grades.reduce((sum, grade) => sum + grade, 0);\r\n      return total / grades.length;\r\n    },\r\n    calculateStatus(student) {\r\n      return this.calculateAverage(student) >= 7 ? 'Approved' : 'Failed';\r\n    },\r\n    editStudent(student) {\r\n      this.currentStudent = { ...student };\r\n      this.showForm = true;\r\n    },\r\n    deleteStudent(studentId) {\r\n      this.students = this.students.filter(student => student.id !== studentId);\r\n    },\r\n    handleSave(student) {\r\n      if (student.id) {\r\n        // Update existing student\r\n        const index = this.students.findIndex(stu => stu.id === student.id);\r\n        this.$set(this.students, index, student);\r\n      } else {\r\n        // Add new student\r\n        student.id = Date.now(); // Simple unique ID\r\n        this.students.push(student);\r\n      }\r\n      this.showForm = false;\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n/* Adicione estilos personalizados aqui */\r\n</style>\r\n"],"mappings":";AAkCA,OAAOA,WAAU,MAAO,mBAAmB;AAE3C,eAAe;EACbC,UAAU,EAAE;IACVD;EACF,CAAC;EACDE,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,KAAK;MACfC,cAAc,EAAE,IAAI;MACpBC,QAAQ,EAAE,EAAE;MACZC,OAAO,EAAE,CACP;QAAEC,IAAI,EAAE,MAAM;QAAEC,KAAK,EAAE;MAAO,CAAC,EAC/B;QAAED,IAAI,EAAE,SAAS;QAAEC,KAAK,EAAE;MAAS,CAAC,EACpC;QAAED,IAAI,EAAE,SAAS;QAAEC,KAAK,EAAE;MAAS,CAAC,EACpC;QAAED,IAAI,EAAE,SAAS;QAAEC,KAAK,EAAE;MAAS,CAAC,EACpC;QAAED,IAAI,EAAE,SAAS;QAAEC,KAAK,EAAE;MAAS,CAAC,EACpC;QAAED,IAAI,EAAE,SAAS;QAAEC,KAAK,EAAE;MAAU,CAAC,EACrC;QAAED,IAAI,EAAE,QAAQ;QAAEC,KAAK,EAAE;MAAS,CAAC,EACnC;QAAED,IAAI,EAAE,SAAS;QAAEC,KAAK,EAAE;MAAU;IAExC,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IA