{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport StudentForm from './StudentForm.vue';\nexport default {\n  components: {\n    StudentForm\n  },\n  data() {\n    return {\n      headers: [{\n        title: 'Nome',\n        value: 'name',\n        class: 'name-column'\n      }, {\n        title: 'Nota 1',\n        value: 'grade1',\n        class: 'grade-column'\n      }, {\n        title: 'Nota 2',\n        value: 'grade2',\n        class: 'grade-column'\n      }, {\n        title: 'Nota 3',\n        value: 'grade3',\n        class: 'grade-column'\n      }, {\n        title: 'Nota 4',\n        value: 'grade4',\n        class: 'grade-column'\n      }, {\n        title: 'Média',\n        value: 'average',\n        class: 'grade-column'\n      }, {\n        title: 'Situação',\n        value: 'status',\n        class: 'status-column'\n      }, {\n        title: 'Ações',\n        value: 'actions',\n        sortable: false,\n        class: 'actions-column'\n      }],\n      students: [],\n      currentStudent: {},\n      formVisible: false\n    };\n  },\n  methods: {\n    showForm(student = {}) {\n      this.currentStudent = {\n        ...student\n      };\n      this.formVisible = true;\n    },\n    closeForm() {\n      this.formVisible = false;\n    },\n    save(student) {\n      if (student.id) {\n        const index = this.students.findIndex(s => s.id === student.id);\n        if (index !== -1) {\n          this.students[index] = student;\n        }\n      } else {\n        student.id = Date.now();\n        this.students.push(student);\n      }\n      this.closeForm();\n    },\n    editStudent(student) {\n      this.showForm(student);\n    },\n    deleteStudent(student) {\n      this.students = this.students.filter(s => s.id !== student.id);\n    },\n    calculateAverage(student) {\n      const grades = [student.grade1, student.grade2, student.grade3, student.grade4].map(Number);\n      const total = grades.reduce((sum, grade) => sum + grade, 0);\n      return (total / grades.length).toFixed(2);\n    },\n    calculateStatus(student) {\n      return this.calculateAverage(student) >= 7 ? 'Approved' : 'Failed';\n    }\n  }\n};","map":{"version":3,"names":["StudentForm","components","data","headers","title","value","class","sortable","students","currentStudent","formVisible","methods","showForm","student","closeForm","save","id","index","findIndex","s","Date","now","push","editStudent","deleteStudent","filter","calculateAverage","grades","grade1","grade2","grade3","grade4","map","Number","total","reduce","sum","grade","length","toFixed","calculateStatus"],"sources":["C:\\Temp\\ws-vscode\\student-score-board\\src\\components\\StudentTable.vue"],"sourcesContent":["<template>\r\n    <div>\r\n        <v-btn @click=\"showForm\">Cadastrar</v-btn>\r\n        <v-data-table :headers=\"headers\" :items=\"students\">\r\n            <template v-slot:body=\"{ items }\">   \r\n                <tbody>\r\n                    <tr v-for=\"item in items\" :key=\"item.id\">\r\n                        <td>{{item.name}}</td>\r\n                        <td>{{item.grade1}}</td>\r\n                        <td>{{item.grade2}}</td>\r\n                        <td>{{item.grade3}}</td>\r\n                        <td>{{item.grade4}}</td>\r\n                        <td>{{calculateAverage(item)}}</td>\r\n                        <td>{{calculateStatus(item)}}</td>\r\n                        <td>\r\n                            <v-icon @click=\"editStudent(item)\">mdi-pencil</v-icon>\r\n                            <v-icon @click=\"deleteStudent(item)\">mdi-delete</v-icon>\r\n                        </td>\r\n                    </tr>\r\n                </tbody>\r\n            </template>\r\n        </v-data-table>\r\n\r\n        <StudentForm \r\n        :student=\"currentStudent\"\r\n        :visible=\"formVisible\"\r\n        @save=\"save\"\r\n        @cancel=\"cancel\"\r\n        />\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n\r\nimport StudentForm from './StudentForm.vue'\r\n\r\nexport default {\r\n   components: {\r\n    StudentForm\r\n   },\r\n   data() {\r\n    return{\r\n        headers: [\r\n            { title: 'Nome', value: 'name', class: 'name-column' },\r\n            { title: 'Nota 1', value: 'grade1', class: 'grade-column' },\r\n            { title: 'Nota 2', value: 'grade2', class: 'grade-column' },\r\n            { title: 'Nota 3', value: 'grade3', class: 'grade-column' },\r\n            { title: 'Nota 4', value: 'grade4', class: 'grade-column' },\r\n            { title: 'Média', value: 'average', class: 'grade-column' },\r\n            { title: 'Situação', value: 'status', class: 'status-column' },\r\n            { title: 'Ações', value: 'actions', sortable: false, class: 'actions-column' }\r\n        ],\r\n        students: [],\r\n        currentStudent: {},\r\n        formVisible: false\r\n    }\r\n   },\r\n   methods: {\r\n    showForm(student = {}) {\r\n        this.currentStudent = {...student}\r\n        this.formVisible = true\r\n    },\r\n    closeForm () {\r\n        this.formVisible = false\r\n    },\r\n    save(student){\r\n        if(student.id) {\r\n            const index = this.students.findIndex(s => s.id === student.id)\r\n            if(index !== -1){\r\n                this.students[index] = student\r\n            }\r\n        } else{\r\n            student.id = Date.now()\r\n            this.students.push(student)\r\n        }\r\n        this.closeForm()\r\n    },\r\n    editStudent(student) {\r\n      this.showForm(student)\r\n    },\r\n    deleteStudent(student) {\r\n      this.students = this.students.filter(s => s.id !== student.id)\r\n    },\r\n    calculateAverage(student) {\r\n      const grades = [student.grade1, student.grade2, student.grade3, student.grade4].map(Number)\r\n      const total = grades.reduce((sum, grade) => sum + grade, 0)\r\n      return (total / grades.length).toFixed(2)\r\n    },\r\n    calculateStatus(student) {\r\n      return this.calculateAverage(student) >= 7 ? 'Approved' : 'Failed'\r\n    }\r\n   }\r\n}\r\n</script>\r\n\r\n<style>\r\n.v-data-table-header th {\r\n  padding: 16px; /* Aumenta o padding nas células do cabeçalho */\r\n}\r\n\r\n.v-data-table tbody td {\r\n  padding: 20px; /* Aumenta o padding nas células do corpo da tabela */\r\n}\r\n\r\n.v-data-table .name-column {\r\n  width: 200px; /* Definir uma largura personalizada */\r\n}\r\n\r\n.v-data-table .grade-column {\r\n  width: 100px;\r\n}\r\n\r\n.v-data-table .status-column {\r\n  width: 150px;\r\n}\r\n</style>"],"mappings":";AAkCA,OAAOA,WAAU,MAAO,mBAAkB;AAE1C,eAAe;EACZC,UAAU,EAAE;IACXD;EACD,CAAC;EACDE,IAAIA,CAAA,EAAG;IACN,OAAM;MACFC,OAAO,EAAE,CACL;QAAEC,KAAK,EAAE,MAAM;QAAEC,KAAK,EAAE,MAAM;QAAEC,KAAK,EAAE;MAAc,CAAC,EACtD;QAAEF,KAAK,EAAE,QAAQ;QAAEC,KAAK,EAAE,QAAQ;QAAEC,KAAK,EAAE;MAAe,CAAC,EAC3D;QAAEF,KAAK,EAAE,QAAQ;QAAEC,KAAK,EAAE,QAAQ;QAAEC,KAAK,EAAE;MAAe,CAAC,EAC3D;QAAEF,KAAK,EAAE,QAAQ;QAAEC,KAAK,EAAE,QAAQ;QAAEC,KAAK,EAAE;MAAe,CAAC,EAC3D;QAAEF,KAAK,EAAE,QAAQ;QAAEC,KAAK,EAAE,QAAQ;QAAEC,KAAK,EAAE;MAAe,CAAC,EAC3D;QAAEF,KAAK,EAAE,OAAO;QAAEC,KAAK,EAAE,SAAS;QAAEC,KAAK,EAAE;MAAe,CAAC,EAC3D;QAAEF,KAAK,EAAE,UAAU;QAAEC,KAAK,EAAE,QAAQ;QAAEC,KAAK,EAAE;MAAgB,CAAC,EAC9D;QAAEF,KAAK,EAAE,OAAO;QAAEC,KAAK,EAAE,SAAS;QAAEE,QAAQ,EAAE,KAAK;QAAED,KAAK,EAAE;MAAiB,EAChF;MACDE,QAAQ,EAAE,EAAE;MACZC,cAAc,EAAE,CAAC,CAAC;MAClBC,WAAW,EAAE;IACjB;EACD,CAAC;EACDC,OAAO,EAAE;IACRC,QAAQA,CAACC,OAAM,GAAI,CAAC,CAAC,EAAE;MACnB,IAAI,CAACJ,cAAa,GAAI;QAAC,GAAGI;MAAO;MACjC,IAAI,CAACH,WAAU,GAAI,IAAG;IAC1B,CAAC;IACDI,SAAQA,CAAA,EAAK;MACT,IAAI,CAACJ,WAAU,GAAI,KAAI;IAC3B,CAAC;IACDK,IAAIA,CAACF,OAAO,EAAC;MACT,IAAGA,OAAO,CAACG,EAAE,EAAE;QACX,MAAMC,KAAI,GAAI,IAAI,CAACT,QAAQ,CAACU,SAAS,CAACC,CAAA,IAAKA,CAAC,CAACH,EAAC,KAAMH,OAAO,CAACG,EAAE;QAC9D,IAAGC,KAAI,KAAM,CAAC,CAAC,EAAC;UACZ,IAAI,CAACT,QAAQ,CAACS,KAAK,IAAIJ,OAAM;QACjC;MACJ,OAAM;QACFA,OAAO,CAACG,EAAC,GAAII,IAAI,CAACC,GAAG,CAAC;QACtB,IAAI,CAACb,QAAQ,CAACc,IAAI,CAACT,OAAO;MAC9B;MACA,IAAI,CAACC,SAAS,CAAC;IACnB,CAAC;IACDS,WAAWA,CAAC