v-card>\r\n    </v-dialog>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n    props: {\r\n        student: Object,\r\n        visible: Boolean\r\n    },\r\n    data() {\r\n        return{\r\n            localStudent: { ...this.student},\r\n            localVisible: this.visible,\r\n            subjects: ['Matemática', 'Português', 'Geografia', 'Ciências', 'Filosofia', 'Inglês'],\r\n            rules: {\r\n                name: value => !!value || 'Este campo é obrigatório',\r\n            }\r\n        }\r\n    },\r\n    watch: {\r\n        visible(newValue) {\r\n            this.localVisible = newValue\r\n        },\r\n        student(newValue){\r\n            this.localStudent = { ...newValue}\r\n        }\r\n    },\r\n    methods: {\r\n        validadeSubject(value) {\r\n            return [\r\n                value => !!value || 'Você deve selecionar uma disciplina',\r\n                value => this.subjects.includes(value) || `Disciplina ${value} inválida`\r\n            ]\r\n        },\r\n        gradesValidation(gradeNumber){\r\n            return [\r\n                value => !!value || `Você deve digitar a nota ${gradeNumber}`,\r\n                value => (value && !isNaN(value) && value >= 0 && value <= 10) || `A nota ${gradeNumber} deve ser um número entre 0 e 10`\r\n            ]\r\n        },\r\n        close() {\r\n            this.$emit('cancel')\r\n        },\r\n        async save() {\r\n            const { valid } = await this.$refs.form.validate()\r\n            if(valid){\r\n                this.$emit('save', this.localStudent)\r\n            }\r\n        },\r\n        outsideClick() {\r\n            this.localVisible = false\r\n        }\r\n    }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.custom-card {\r\n    border-radius: 76px;\r\n}\r\n.card-title {\r\n    text-align: center;\r\n    width: 100%; \r\n    display: block; \r\n    margin-bottom: 16px; \r\n    font-size: 20px; \r\n    font-weight: bold; \r\n}\r\n.save-button {\r\n    backgrou